<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="login-nav">
  <a href="/">
    Back
  </a>
  <p>
    LOGIN
  </p>
  <div class="actions">
    <%= f.submit "Log In" %>
  </div>
</div>


  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email",class: "email" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password" %>
  </div>

  

<% end %>

<div class="links">
<%= render "devise/shared/links" %>

</div>
