
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

<div class="login-nav">
    <%= link_to "Back", new_user_session_path %>
  <p>
    REGISTER
  </p>
  <div class="actions">
    <%= f.submit "Next" %>
  </div>
</div>
  <%= render "devise/shared/error_messages", resource: resource %>

   <div class="field">
    <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "Name",class: "email"  %>
  </div>

  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email",class: "email" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "new-password", placeholder: "Password"%>
  </div>



<% end %>

